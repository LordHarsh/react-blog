{
  "version": 2,
  "builds": [
    {
      "src": "server.js",
      "use": "@vercel/node"
    },
    {
      "src": "package.json",
      "use": {
        "command": "npm install && npm run build",
        "env": {
          "CI": "1",
          "NODE_ENV": "production"
        }
      }
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/server.js"
    },
    {
      "src": "/(.*)",
      "dest": "/"
    }
  ]
}
